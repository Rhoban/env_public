      <preamble>self.fallback = False
self.fallface = False
self.releve = False
self.playing = True

def checkGameState(self):
 info = self.SchedulerGetOutputValues_response("GameStatusTracker",["Playing"])
 if info and len(info) &gt;= 1 and len(info[0]) == 1:
  self.playing = (info[0][0] == 1)

def check_fall(self):
 info = self.GetMoveValues("FallDetector",["fallback","fallface"])
 if info and len(info) &gt;= 1 and len(info[0]) &gt;= 2:
  self.fallback = float(info[0][0]) &gt; 0.1
  self.fallface = float(info[0][1]) &gt; 0.1

def check_releve(self, moveName):
 info = self.SchedulerGetOutputValues_response(moveName,["over"])
 if info and len(info) &gt;= 1 and len(info[0])&gt;=1:
  self.releve = float(info[0][0]) &gt; 0.1

