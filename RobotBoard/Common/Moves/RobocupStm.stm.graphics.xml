      <preamble>fallback = False
fallface = False
releve = False
retourne = False
playing = False

class RobocupStm:
    @staticmethod
    def check_fall(connection):
        global fallback
        global fallface
        info = connection.SchedulerGetOutputValues_response("FallDetector",["fallback","fallface"])
        if info and len(info) &gt;= 1 and len(info[0]) &gt;= 2:
             print(info)
             fallback = info[0][0] &gt; 0.1
             fallface = info[0][1] &gt; 0.1
             print(str(fallback) + " " + str(fallface))
        else:
            print("Failed to get message " + str(info))

    @staticmethod
    def check_releve(connection):
        global releve
        info = connection.SchedulerGetOutputValues_response("Relevage",["Finished"])
        if info and len(info) &gt;= 1 and len(info[0])&gt;=1:
            print( str( type( info[0][0] )))
            print( str( info[0][0] ))
            releve = info[0][0] &gt; 0.1
            print( str(releve))

    @staticmethod
    def checkGameState(connection):
        global playing
        info = connection.SchedulerGetOutputValues_response("GameStatusTracker",["Playing"])
        # There should always be one value in info[0] debug is needed if not
        if info and len(info) &gt;= 1 and len(info[0]) == 1:
            playing = info[0][0] == 1
        else:
            print("Failed to get message " + str(info))</preamble>
