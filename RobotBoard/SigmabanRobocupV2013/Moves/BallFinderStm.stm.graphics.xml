          <fire />
        </BlockPredecessor>
        <BlockPredecessor>
          <name>TrackBall</name>
          <output_idx>0</output_idx>
          <condition>not isball</condition>
          <fire />
        </BlockPredecessor>
      </entries>
      <name>LookAround</name>
      <gain>1</gain>
      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <loop>BallFinderStmClass.update_ballinfo(self.connection)</loop>
      <enter>self.connection.SchedulerStartMove("LookAround",0,50)
self.connection.SchedulerUpdateConstant(marche, "Forward", [0])
self.connection.SchedulerUpdateConstant(marche, "Rotation", [0.3])
</enter>
