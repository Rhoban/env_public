  <GraphicalBlock>
    <StmPreambleBlock>
      <entries />
      <name>StmPreambleBlock</name>
      <gain>1</gain>
      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <preamble>x = 0
y = 0
isball = False
radius = 0
chrono = 0
marche = "MarcheBasse"

class BallFinderStmClass:
    @staticmethod
    def update_ballinfo(connection):
        global x
        global y
        global isball
        global radius
        info = connection.VisionGetBallInfo_response(True)
        if info:
            isball = info[0]
            x = info[1]
            y = info[2]
            radius = info[3]
</preamble>
    </StmPreambleBlock>
    <Position>
      <X>-174.05400614146097</X>
      <Y>-11.001590908829911</Y>
    </Position>
  </GraphicalBlock>
  <GraphicalBlock>
    <StmStateBlock>
      <entries>
        <BlockPredecessor>
          <name>Initial</name>
          <output_idx>0</output_idx>
          <condition>isball</condition>
          <fire />
        </BlockPredecessor>
        <BlockPredecessor>
          <name>LookAround</name>
          <output_idx>0</output_idx>
          <condition>isball</condition>
          <fire />
        </BlockPredecessor>
      </entries>
      <name>TrackBall</name>
      <gain>1</gain>
      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <loop>BallFinderStmClass.update_ballinfo(self.connection)
self.connection.SchedulerUpdateConstant("MarcheBasse", "Rotation", [0.8*x]) </loop>
      <enter>self.connection.SchedulerStartMove("TrackBall",0,20)
self.connection.SchedulerUpdateConstant(marche, "Forward", [0.8])
self.connection.SchedulerUpdateConstant(marche, "Rotation", [0])

</enter>
      <exit>self.connection.SchedulerStopMove("TrackBall",50)
</exit>
    </StmStateBlock>
    <Position>
      <X>675.15119804813094</X>
      <Y>2.3840380181724328</Y>
    </Position>
  </GraphicalBlock>
  <GraphicalBlock>
    <StmLoopBlock>
      <entries />
      <name>Initial</name>
      <gain>1</gain>
      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <loop>self.connection.SchedulerStartMove(marche,0,20)
</loop>
    </StmLoopBlock>
    <Position>
      <X>221.94088904861798</X>
      <Y>257.82764211845165</Y>
