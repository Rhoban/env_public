      <gainMax>60</gainMax>
      <loop>RobocupStm.check_fall(self.connection)</loop>
      <enter>self.connection.SchedulerStartMove("WalkToBall",0,100)</enter>
      <exit>self.connection.SchedulerStartMove("WalkToBall",0,100)</exit>
    </StmStateBlock>
