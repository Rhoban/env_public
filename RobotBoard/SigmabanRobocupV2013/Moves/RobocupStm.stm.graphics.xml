  <GraphicalBlock>
    <StmPreambleBlock>
      <entries />
      <name>StmPreambleBlock</name>
      <gain>1</gain>
      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <preamble>fallback = False
fallface = False

class RobocupStm:
    @staticmethod
    def check_fall(connection):
        info = connection.SchedulerGetMoveValues("FallDetector","Result")
        if info:
             fallback = info[0]
             fallface = info[1]
</preamble>
    </StmPreambleBlock>
    <Position>
      <X>-200</X>
      <Y>102</Y>
    </Position>
  </GraphicalBlock>
  <GraphicalBlock>
    <StmStateBlock>
      <entries>
        <BlockPredecessor>
          <name>Initial</name>
          <output_idx>0</output_idx>
          <condition>not fallback and not fallface</condition>
          <fire />
        </BlockPredecessor>
        <BlockPredecessor>
          <name>FallFace</name>
          <output_idx>0</output_idx>
          <condition>finished</condition>
          <fire />
        </BlockPredecessor>
      </entries>
      <name>WalkToBall</name>
      <gain>1</gain>
      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <loop>RobocupStm.check_fall(self.connection)
in_position = ! connection.SchedulerGetMoveValues("WalkToBall","CloseToBall")[0]</loop>
      <enter>self.connection.SchedulerStartMove("MarcheBasse",0,1000)
self.connection.SchedulerStartMove("TrackBall",0,100)
self.connection.SchedulerStartMove("WalkToBall",0,100)
</enter>
      <exit>self.connection.SchedulerStopMove("WalkToBall",100)
self.connection.SchedulerStopMove("TrackBall",100)
self.connection.SchedulerStopMove("MarcheBasse",100)
</exit>
    </StmStateBlock>
    <Position>
