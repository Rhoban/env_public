      <gainMin>-1</gainMin>
      <gainMax>60</gainMax>
      <loop>RobocupStm.check_fall(self.connection)</loop>
      <enter>self.connection.SchedulerStartMove("MarcheBasse",0,1000)
self.connection.SchedulerStartMove("TrackBall",0,100)
self.connection.SchedulerStartMove("WalkToBall",0,100)
