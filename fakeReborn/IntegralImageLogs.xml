<?xml version="1.0" ?><LocalisationTest>
<ballResampling>0.0</ballResampling>
<angularPitchTolerance>2</angularPitchTolerance>
<benchmark>false</benchmark>
<benchmarkDetail>4</benchmarkDetail>
<gpuOn>false</gpuOn>
<readFromLog>true</readFromLog>
<pathToLog>workingLog/lowLevel.log</pathToLog>
<embedded>false</embedded>
<fieldResampling>0.002</fieldResampling>
<imageDelay>11</imageDelay>
<nbParticlesBF>1</nbParticlesBF>
<nbParticlesFF>1000</nbParticlesFF>
<pipeline>
  <filter>
    <className>PatchRecorder</className>
    <name>ballPatchRecorder</name>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>ballPatchProvider</dependency>
    </dependencies>
    <prefix>patches/ball/</prefix>
    <paramInts>
      <paramInt>
        <name>period</name>
        <value>0</value>
      </paramInt>
    </paramInts>
  </filter>
  <filter>
    <className>PatchRecorder</className>
    <name>goalPatchRecorder</name>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>goalPatchProvider</dependency>
    </dependencies>
    <prefix>patches/goal/</prefix>
    <paramInts>
      <paramInt>
        <name>period</name>
        <value>0</value>
      </paramInt>
    </paramInts>
  </filter>
  <filter>
    <className>RoiToPatches</className>
    <name>ballPatchProvider</name>
    <debugLevel>
      <graphics>true</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>human</dependency>
      <dependency>berlinROIProvider</dependency>
    </dependencies>
	  <resizePatches>false</resizePatches>
  </filter>
  <filter>
    <className>RoiToPatches</className>
    <name>goalPatchProvider</name>
    <debugLevel>
      <graphics>true</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>human</dependency>
      <dependency>goalROIProvider</dependency>
    </dependencies>
	  <resizePatches>false</resizePatches>
  </filter>
  <filter>
    <className>GoalByII</className>
    <name>goalROIProvider</name>
    <debugLevel>
      <graphics>true</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>integralY</dependency>
      <dependency>ballRadiusProvider</dependency>
    </dependencies>
    <paramFloats>
      <paramFloat>
	      <name>aboveRatio</name>
	      <value>2.0</value>
      </paramFloat>
      <paramFloat>
	      <name>belowRatio</name>
	      <value>2.0</value>
      </paramFloat>
      <paramFloat>
	      <name>boundaryWidthRatio</name>
	      <value>2</value>
      </paramFloat>
      <paramFloat>
	      <name>widthScale</name>
	      <value>1.0</value>
      </paramFloat>
      <paramFloat>
	      <name>belowCoeff</name>
	      <value>4.0</value>
      </paramFloat>
    </paramFloats>
  </filter>
  <filter>
    <className>BerlinROIProvider</className>
    <name>berlinROIProvider</name>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>integralY</dependency>
      <dependency>ballRadiusProvider</dependency>
    </dependencies>
    <paramFloats>
      <paramFloat>
	      <name>boundaryFactor</name>
	      <value>2.0</value>
      </paramFloat>
    </paramFloats>
  </filter>
  <filter>
    <className>Integral</className>
    <name>integralY</name>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>Y</dependency>
    </dependencies>
  </filter>
  <filter>
    <className>ChannelSelector</className>
    <name>Y</name>
    <debugLevel>
      <graphics>true</graphics>
    </debugLevel>
    <dependencies>
      <dependency>sourceSmall</dependency>
    </dependencies>
    <paramInts>
      <paramInt>
	      <name>channel</name>
	      <value>0</value>
      </paramInt>
    </paramInts>
    <paramFloats/>
    <paramPairInt/>
  </filter>
  <filter>
    <className>ColorConverter</className>
    <name>human</name>
    <conversion>YUV2BGR</conversion>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>source</dependency>
    </dependencies>
  </filter>
  <filter>
    <className>BallRadiusProvider</className>
    <name>ballRadiusProvider</name>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>sourceSmall</dependency>
    </dependencies>
  </filter>
  <filter>
    <className>Rescale</className>
    <name>sourceSmall</name>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies>
      <dependency>source</dependency>
    </dependencies>
    <paramFloats>
      <paramFloat>
	      <name>ratio</name>
	      <value>4.0</value>
      </paramFloat>
    </paramFloats>
  </filter>
  <filter>
    <className>SourceLogs2</className>
    <debugLevel>
      <graphics>false</graphics>
      <parameters>false</parameters>
      <perfs>false</perfs>
      <threads>false</threads>
    </debugLevel>
    <dependencies/>
    <imagesFile>workingLog/images.csv</imagesFile>
    <name>source</name>
    <paramFloats/>
    <paramInts/>
    <paramPairInt/>
    <startIndex>0</startIndex>
  </filter>
</pipeline>
<playing>false</playing>
<showRobotView>false</showRobotView>
<showTaggedImg>false</showTaggedImg>
<showTopView>false</showTopView>
</LocalisationTest>
