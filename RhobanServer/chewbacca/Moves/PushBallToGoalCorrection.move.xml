<?xml version="1.0"?>
<MotorPrimitive xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <entries />
  <name>PushBallToGoalCorrection</name>
  <gain>1</gain>
  <gainMin>-1</gainMin>
  <gainMax>60</gainMax>
  <MoveOutput>
    <entries>
      <BlockPredecessor>
        <name>BlockAddition2</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>MoveOutput</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <labels>
      <string>cap</string>
    </labels>
  </MoveOutput>
  <MoveBlock>
    <entries />
    <name>MoveBlock</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <move_name>LocateBallAndGoal</move_name>
    <inputs />
    <outputs>
      <string>goal_left</string>
      <string>goal_right</string>
      <string>ball_pan</string>
    </outputs>
    <default_outputs>
      <float>0</float>
      <float>0</float>
      <float>0</float>
    </default_outputs>
  </MoveBlock>
  <CommentsBlock>
    <entries />
    <name>CommentsBlock</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <Commentaires>if the ball_pan is less or greater than goal_left or goal_right and if the robot is close  to the ball then the cap is corrected</Commentaires>
  </CommentsBlock>
  <BlockGain>
    <entries>
      <BlockPredecessor>
        <name>MoveBlock</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>MoveBlock</name>
        <output_idx>2</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockGain</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <gains>
      <float>-1</float>
      <float>1</float>
    </gains>
  </BlockGain>
  <BlockGain>
    <entries>
      <BlockPredecessor>
        <name>MoveBlock</name>
        <output_idx>1</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>MoveBlock</name>
        <output_idx>2</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockGain1</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <gains>
      <float>1</float>
      <float>-1</float>
    </gains>
  </BlockGain>
  <BlockAddition>
    <entries>
      <BlockPredecessor>
        <name>BlockGain</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>BlockGain</name>
        <output_idx>1</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockAddition</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
  </BlockAddition>
  <BlockAddition>
    <entries>
      <BlockPredecessor>
        <name>BlockGain1</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>BlockGain1</name>
        <output_idx>1</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockAddition1</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
  </BlockAddition>
  <BlockDigitalLevel>
    <entries>
      <BlockPredecessor>
        <name>BlockAddition</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockDigitalLevel</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <min_value>
      <float>0</float>
    </min_value>
    <max_value>
      <float>0</float>
    </max_value>
    <output_low>10</output_low>
    <output_medium>0</output_medium>
    <output_high>0</output_high>
    <hysteresis>false</hysteresis>
  </BlockDigitalLevel>
  <BlockDigitalLevel>
    <entries>
      <BlockPredecessor>
        <name>BlockAddition1</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockDigitalLevel1</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <min_value>
      <float>0</float>
    </min_value>
    <max_value>
      <float>0</float>
    </max_value>
    <output_low>-10</output_low>
    <output_medium>0</output_medium>
    <output_high>0</output_high>
    <hysteresis>false</hysteresis>
  </BlockDigitalLevel>
  <BlockAddition>
    <entries>
      <BlockPredecessor>
        <name>BlockDigitalLevel</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>BlockDigitalLevel1</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>BlockAddition2</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
  </BlockAddition>
  <SchemaComments />
  <frequency>100</frequency>
</MotorPrimitive>