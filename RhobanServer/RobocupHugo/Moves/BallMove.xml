<?xml version="1.0"?>
<MotorPrimitive xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <entries />
  <name>BallMove</name>
  <gain>1</gain>
  <gainMin>-1</gainMin>
  <gainMax>60</gainMax>
  <CommentsBlock>
    <entries />
    <name>CommentsBlock</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <Commentaires>Moves the head to the ball when the ball is detected</Commentaires>
  </CommentsBlock>
  <BallBlock>
    <entries />
    <name>BallBlock</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <refresh_frequency>20</refresh_frequency>
    <clipping>true</clipping>
  </BallBlock>
  <StmWriteBlock>
    <entries>
      <BlockPredecessor>
        <name>BallBlock</name>
        <output_idx>0</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>BallBlock</name>
        <output_idx>1</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>BallBlock</name>
        <output_idx>2</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
      <BlockPredecessor>
        <name>BallBlock</name>
        <output_idx>3</output_idx>
        <condition />
        <fire />
      </BlockPredecessor>
    </entries>
    <name>StmWriteBlock</name>
    <gain>1</gain>
    <gainMin>-1</gainMin>
    <gainMax>60</gainMax>
    <machine_name>BallStm</machine_name>
    <refresh_frequency>10</refresh_frequency>
    <attributes>
      <string>isBallDetected</string>
      <string>x</string>
      <string>y</string>
      <string>radius</string>
    </attributes>
  </StmWriteBlock>
  <SchemaComments />
  <frequency>100</frequency>
</MotorPrimitive>