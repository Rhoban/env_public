<?xml version="1.0"?>
<LocalisationTest>
<ballResampling>0.0</ballResampling>
<angularPitchTolerance>2</angularPitchTolerance>
<benchmark>false</benchmark>
<benchmarkDetail>0</benchmarkDetail>
<gpuOn>false</gpuOn>
<readFromLog>false</readFromLog>
<pathToLog>Logs/leipz1/lowLevel.log</pathToLog>
<embedded>true</embedded>
<fieldResampling>0.002</fieldResampling>
<imageDelay>11</imageDelay>
<nbParticlesBF>1</nbParticlesBF>
<nbParticlesFF>1000</nbParticlesFF>
<pipeline>
  <filter>
    <className>ColorConverter</className>
    <name>human</name>
    <conversion>YUV2BGR</conversion>
    <debugLevel>
      <graphics>false</graphics>
    </debugLevel>
    <dependencies>
      <dependency>source</dependency>
    </dependencies>
    <paramInts/>
    <paramFloats/>
    <paramPairInt/>
  </filter>
  <filter>
    <className>ColorBounding</className>
    <name>ballColor</name>
    <dependencies>
      <dependency>source</dependency>
    </dependencies>
    <debugLevel>
      <graphics>false</graphics>
    </debugLevel>
    <ParamInts>
      <ParamInt>
        <name>minY</name>
        <val>100</val>
      </ParamInt>
      <ParamInt>
        <name>maxY</name>
        <val>150</val>
      </ParamInt>
      <ParamInt>
        <name>minU</name>
        <val>100</val>
      </ParamInt>
      <ParamInt>
        <name>maxU</name>
        <val>150</val>
      </ParamInt>
      <ParamInt>
        <name>minV</name>
        <val>100</val>
      </ParamInt>
      <ParamInt>
        <name>maxV</name>
        <val>150</val>
      </ParamInt>
    </ParamInts>
  </filter>
  <filter>
    <className>SourcePtGrey</className>
    <name>source</name>
    <debugLevel>
      <graphics>false</graphics>
    </debugLevel>
    <wished_properties>
      <Shutter>
        <autoManualMode>false</autoManualMode>
        <absValue>5</absValue>
      </Shutter>
      <Gain>
        <autoManualMode>false</autoManualMode>
        <absValue>10</absValue>
      </Gain>
      <FrameRate>
        <autoManualMode>false</autoManualMode>
        <absValue>25</absValue>
      </FrameRate>
    </wished_properties>
    <dependencies/>
    <paramInts/>
    <paramFloats/>
    <paramPairInt/>
  </filter>
</pipeline>
<playing>true</playing>
<showRobotView>false</showRobotView>
<showTaggedImg>false</showTaggedImg>
<showTopView>false</showTopView>
</LocalisationTest>
