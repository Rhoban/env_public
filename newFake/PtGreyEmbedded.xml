        <ParamInt>
          <name>minY</name>
          <value>53</value>
        </ParamInt>
        <ParamInt>
          <name>maxY</name>
          <value>130</value>
        </ParamInt>
        <ParamInt>
          <name>minU</name>
          <value>113</value>
        </ParamInt>
        <ParamInt>
          <name>maxU</name>
          <value>130</value>
        </ParamInt>
        <ParamInt>
          <name>minV</name>
          <value>109</value>
        </ParamInt>
        <ParamInt>
          <name>maxV</name>
          <value>129</value>
        </ParamInt>
      </paramInts>
    </filter>
    <filter>
      <className>SourcePtGrey</className>
      <name>source</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <wished_properties>
        <Shutter>
          <autoManualMode>false</autoManualMode>
          <absValue>5</absValue>
        </Shutter>
        <Gain>
          <autoManualMode>false</autoManualMode>
          <absValue>10</absValue>
        </Gain>
        <FrameRate>
          <autoManualMode>false</autoManualMode>
          <absValue>25</absValue>
        </FrameRate>
      </wished_properties>
      <dependencies/>
      <paramInts/>
      <paramFloats/>
      <paramPairInt/>
    </filter>
  </pipeline>
  <playing>true</playing>
  <showRobotView>false</showRobotView>
  <showTaggedImg>false</showTaggedImg>
  <showTopView>false</showTopView>
</LocalisationTest>
