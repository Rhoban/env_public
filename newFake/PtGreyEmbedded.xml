      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>fieldDilated</dependency>
        <dependency>binaryBallDilated</dependency>
      </dependencies>
    </filter>
    <filter>
      <className>Dilate</className>
      <name>fieldDilated</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>fieldColor</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>kernel width</name>
	        <value>5</value>
        </paramInt>
        <paramInt>
	        <name>kernel height</name>
	        <value>5</value>
        </paramInt>
      </paramInts>
      <paramFloats/>
      <paramPairInt/>
    </filter>
    <filter>
      <className>Threshold</className>
