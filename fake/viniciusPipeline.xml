<?xml version="1.0" ?>
<LocalisationTest>
  <ballResampling>0.0</ballResampling>
  <angularPitchTolerance>2</angularPitchTolerance>
  <benchmark>false</benchmark>
  <benchmarkDetail>4</benchmarkDetail>
  <gpuOn>false</gpuOn>
  <readFromLog>true</readFromLog>
  <pathToLog>workingLog/lowLevel.log</pathToLog>
  <embedded>false</embedded>
  <fieldResampling>0.0</fieldResampling>
  <imageDelay>-5</imageDelay>
  <nbParticlesBF>1</nbParticlesBF>
  <nbParticlesFF>500</nbParticlesFF>
  <arenaProviders/>
  <ballProviders/>
  <goalProviders/>
  <compassProviders/>
  <pipeline>
    <filter>
      <className>RobotDetector</className>
      <name>magentaPlayers</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>magenta</dependency>
        <dependency>green</dependency>
      </dependencies>
    </filter>
    <filter>
      <className>RobotDetector</className>
      <name>cyanPlayers</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>cyan</dependency>
        <dependency>green</dependency>
      </dependencies>
    </filter>
    <filter>
      <className>ColorBounding</className>
      <name>green</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>hsv</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>minY</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxY</name>
	        <value>255</value>
        </paramInt>
        <paramInt>
	        <name>minU</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxU</name>
	        <value>255</value>
        </paramInt>
        <paramInt>
	        <name>minV</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxV</name>
	        <value>255</value>
        </paramInt>
      </paramInts>
    </filter>
    <filter>
      <className>ColorBounding</className>
      <name>magenta</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>hsv</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>minY</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxY</name>
	        <value>255</value>
        </paramInt>
        <paramInt>
	        <name>minU</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxU</name>
	        <value>255</value>
        </paramInt>
        <paramInt>
	        <name>minV</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxV</name>
	        <value>255</value>
        </paramInt>
      </paramInts>
    </filter>
    <filter>
      <className>ColorBounding</className>
      <name>cyan</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>hsv</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>minY</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxY</name>
	        <value>255</value>
        </paramInt>
        <paramInt>
	        <name>minU</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxU</name>
	        <value>255</value>
        </paramInt>
        <paramInt>
	        <name>minV</name>
	        <value>0</value>
        </paramInt>
        <paramInt>
	        <name>maxV</name>
	        <value>255</value>
        </paramInt>
      </paramInts>
    </filter>
    <filter>
      <className>ColorConverter</className>
      <name>hsv</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>bgr</dependency>
      </dependencies>
      <conversion>BGR2HSV</conversion>
    </filter>
    <filter>
      <className>ColorConverter</className>
      <name>bgr</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>smallSource</dependency>
      </dependencies>
      <conversion>YUV2BGR</conversion>
    </filter>
    <filter>
      <className>Rescale</className>
      <name>smallSource</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>source</dependency>
      </dependencies>
      <paramFloats>
        <paramFloat>
	        <name>ratio</name>
	        <value>2</value>
        </paramFloat>
      </paramFloats>
    </filter>
    <filter>
      <className>SourceLogs2</className>
      <name>source</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies/>
      <imagesFile>workingLog/images.csv</imagesFile>
      <paramFloats/>
      <paramInts/>
      <paramPairInt/>
      <startIndex>0</startIndex>
    </filter>
  </pipeline>
  <playing>false</playing>
  <showRobotView>false</showRobotView>
  <showTaggedImg>false</showTaggedImg>
  <showTopView>false</showTopView>
</LocalisationTest>
