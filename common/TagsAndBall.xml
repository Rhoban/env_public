<?xml version="1.0" ?>
<LocalisationTest>
  <angularPitchTolerance>5</angularPitchTolerance>
  <useTags>true</useTags>
  <benchmark>false</benchmark>
  <benchmarkDetail>4</benchmarkDetail>
  <gpuOn>false</gpuOn>
  <readFromLog>false</readFromLog>
  <embedded>true</embedded>
  <imageDelay>-5</imageDelay>
  <pipeline>
    <filter>
      <className>BallByDNN</className>
      <name>ballByDNN</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>ballPatchProvider</dependency>
      </dependencies>
      <arch_path>../common/dnn/balls/structure.json</arch_path>
      <weights_path>../common/dnn/balls/weights.bin</weights_path>
      <paramFloats>
        <paramFloat>
	        <name>scoreThreshold</name>
	        <value>0.85</value>
        </paramFloat>
      </paramFloats>
    </filter>
    <filter>
      <className>RoiToPatches</className>
      <name>ballPatchProvider</name>
      <debugLevel>
        <graphics>false</graphics>
        <parameters>false</parameters>
        <perfs>false</perfs>
        <threads>false</threads>
      </debugLevel>
      <dependencies>
        <dependency>human</dependency>
        <dependency>ballByII</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
          <name>tagLevel</name>
          <value>1</value>
        </paramInt>
        <paramInt>
          <name>patchHeight</name>
          <value>64</value>
        </paramInt>
        <paramInt>
          <name>patchWidth</name>
          <value>64</value>
        </paramInt>
      </paramInts>
    </filter>
    <filter>
      <className>BallByII2</className>
      <name>ballByII</name>
      <debugLevel>
        <graphics>true</graphics>
        <parameters>false</parameters>
        <perfs>false</perfs>
        <threads>false</threads>
      </debugLevel>
      <dependencies>
        <dependency>integralY</dependency>
        <dependency>greenII</dependency>
        <dependency>ballRadiusProvider</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>decimationRate</name>
	        <value>5</value>
        </paramInt>
      </paramInts>
      <paramFloats>
        <paramFloat>
	        <name>boundaryFactor</name>
	        <value>2.0</value>
        </paramFloat>
        <paramFloat>
	        <name>yWeight</name>
	        <value>5.0</value>
        </paramFloat>
        <paramFloat>
	        <name>greenWeight</name>
	        <value>6.0</value>
        </paramFloat>
        <paramFloat>
	        <name>minScore</name>
	        <value>60</value>
        </paramFloat>
        <paramFloat>
	        <name>minRadius</name>
	        <value>10</value>
        </paramFloat>
      </paramFloats>
    </filter>
    <filter>
      <className>Integral</className>
      <name>integralY</name>
      <debugLevel>
        <graphics>false</graphics>
        <parameters>false</parameters>
        <perfs>false</perfs>
        <threads>false</threads>
      </debugLevel>
      <dependencies>
        <dependency>Y</dependency>
      </dependencies>
    </filter> 
    <filter>
      <className>ChannelSelector</className>
      <name>Y</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>source</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>channel</name>
	        <value>0</value>
        </paramInt>
      </paramInts>
      <paramFloats/>
      <paramPairInt/>
    </filter> 
    <filter>
      <className>Integral</className>
      <name>greenII</name>
      <debugLevel>
        <graphics>false</graphics>
        <parameters>false</parameters>
        <perfs>false</perfs>
        <threads>false</threads>
      </debugLevel>
      <dependencies>
        <dependency>green</dependency>
      </dependencies>
    </filter>
    <filter>
      <className>ColorBounding</className>
      <name>green</name>
      <debugLevel>
        <graphics>true</graphics>
        <parameters>false</parameters>
        <perfs>false</perfs>
        <threads>false</threads>
      </debugLevel>
      <dependencies>
        <dependency>source</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>minY</name>
	        <value>40</value>
        </paramInt>
        <paramInt>
	        <name>minU</name>
	        <value>112</value>
        </paramInt>
        <paramInt>
	        <name>minV</name>
	        <value>117</value>
        </paramInt>
        <paramInt>
	        <name>maxY</name>
	        <value>110</value>
        </paramInt>
        <paramInt>
	        <name>maxU</name>
	        <value>131</value>
        </paramInt>
        <paramInt>
	        <name>maxV</name>
	        <value>131</value>
        </paramInt>
      </paramInts>
    </filter>
    <filter>
      <className>TagsDetector</className>
      <name>tagsDetector</name>
      <debugLevel>
        <graphics>true</graphics>
      </debugLevel>
      <dependencies>
        <dependency>Y</dependency>
      </dependencies>
      <paramFloats>
        <paramFloat>
          <name>markerSize</name>
          <value>0.2</value>
        </paramFloat>
      </paramFloats>
    </filter> 
    <filter>
      <className>ChannelSelector</className>
      <name>Y</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>source</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>channel</name>
	        <value>0</value>
        </paramInt>
      </paramInts>
      <paramFloats/>
      <paramPairInt/>
    </filter>
    <filter>
      <className>SourcePtGrey</className>
      <name>source</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <wished_properties>
        <Shutter>
          <autoManualMode>false</autoManualMode>
          <absValue>3</absValue>
        </Shutter>
        <Gain>
          <autoManualMode>false</autoManualMode>
          <absValue>10</absValue>
        </Gain>
        <FrameRate>
          <autoManualMode>false</autoManualMode>
          <absValue>10</absValue>
        </FrameRate>
      </wished_properties>
      <dependencies/>
      <paramInts/>
      <paramFloats/>
      <paramPairInt/>
    </filter>
  </pipeline>
  <playing>true</playing>
  <showRobotView>false</showRobotView>
  <showTaggedImg>false</showTaggedImg>
  <showTopView>false</showTopView>
</LocalisationTest>
