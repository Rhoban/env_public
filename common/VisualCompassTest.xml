<?xml version="1.0" ?>
<LocalisationTest>
  <angularPitchTolerance>5</angularPitchTolerance>
  <benchmark>true</benchmark>
  <benchmarkDetail>4</benchmarkDetail>
  <gpuOn>false</gpuOn>
  <readFromLog>false</readFromLog>
  <embedded>true</embedded>
  <imageDelay>-5</imageDelay>
  <arenaProviders/>
  <goalProviders/>
  <ballProviders/>

  <pipeline>


    <filter>
      <className>VisualCompass</className>
      <name>VisualCompass</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <paramInts>
        <paramInt>
	        <name>debugLevel</name>
	        <value>1</value>
        </paramInt>
        <paramInt>
	        <name>mode</name>
	        <value>0</value>
        </paramInt>
      </paramInts>
      <paramFloats>
	     <paramFloat>
              <name>threshold</name>
	        <value>70</value>
	     </paramFloat>
	     </paramFloats>
      <dependencies>
        <dependency>Y</dependency>
      </dependencies>
    </filter>


    <filter>
      <className>ChannelSelector</className>
      <name>Y</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <dependencies>
        <dependency>source</dependency>
      </dependencies>
      <paramInts>
        <paramInt>
	        <name>channel</name>
	        <value>0</value>
        </paramInt>
      </paramInts>
      <paramFloats/>
      <paramPairInt/>
    </filter>


    <filter>
      <className>SourcePtGrey</className>
      <name>source</name>
      <debugLevel>
        <graphics>false</graphics>
      </debugLevel>
      <wished_properties>
        <Shutter>
          <autoManualMode>false</autoManualMode>
          <absValue>3</absValue>
        </Shutter>
        <Gain>
          <autoManualMode>false</autoManualMode>
          <absValue>8</absValue>
        </Gain>
        <FrameRate>
          <autoManualMode>false</autoManualMode>
          <absValue>10</absValue>
        </FrameRate>
      </wished_properties>
      <dependencies/>
      <paramInts/>
      <paramFloats/>
      <paramPairInt/>
    </filter>


  </pipeline>
  <playing>true</playing>
  <showRobotView>false</showRobotView>
  <showTaggedImg>false</showTaggedImg>
  <showTopView>false</showTopView>
</LocalisationTest>
