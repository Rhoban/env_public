{
    "angularPitchTolerance" : 5,
    "benchmark" : false,
    "benchmarkDetail" : 4,
    "gpuOn" : false,
    "readFromLog" : true,
    "embedded" : false,
    "imageDelay" : -5,
    "arenaProviders" : [],
    "goalProviders" : [],
    "ballProviders" : [],
    "compassProviders" : [],
    "tagsProviders" : ["tagsDetector"],
    "playing" : false,
    "pathToLog" : "workingLog/lowLevel.log",
    "pipeline" :
    [
        {
            "class name" : "TagsDetector",
            "content" : {
                "name" : "tagsDetector",
                "display" : true,
                "dependencies" : ["Y"],
                "paramInts" : {
                    "debugLevel" : 1,
                    "isWritingData" : 1
                },
                "paramFloats" : {
                    "markerSize" : 0.09
                }
            }
        },
        {
            "class name" : "ChannelSelector",
            "content" : {
                "name" : "Y",
                "dependencies" : ["source"],
                "paramInts" : {
                    "channel" : 0
                }
            }
        },
        {
            "class name" : "SourceLogs",
            "content" : {
                "name" : "source",
                "imagesFile" : "workingLog/images.csv",
                //            "display" : true,
                "startIndex" : 0
            }
        }
    ]
}
